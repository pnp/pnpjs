// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "coverage": true,
        ".nyc_output": true,
        "build": true,
        "dist": true,
        "site": true,
        "buildsystem-config.js": true
    },
    "typescript.validate.enable": true,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "editor.tabSize": 4,
    "cSpell.words": [
        "deleteable",
        "graphfi",
        "graphqueryable",
        "Invokable",
        "queryables"
    ],
}